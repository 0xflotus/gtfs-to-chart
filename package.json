{
  "name": "gtfs-to-chart",
  "description": "Generate stringline charts of a transit routes from GTFS",
  "version": "1.0.0",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "visualizations",
    "stringline",
    "d3"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "repository": "git://github.com/blinktaginc/gtfs-to-chart",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-chart/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "bin": {
    "gtfs-to-chart": "bin/gtfs-to-chart.js"
  },
  "main": "index",
  "dependencies": {
    "chalk": "^4.1.0",
    "connect-slashes": "^1.4.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "gtfs": "^2.0.0",
    "js-beautify": "^1.11.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "progress": "^2.0.3",
    "pug": "^3.0.0",
    "sanitize-filename": "^1.6.3",
    "simplify-geojson": "^1.0.3",
    "sqlstring": "^2.3.2",
    "timer-machine": "^1.1.0",
    "untildify": "^4.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "xo": "^0.32.1"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "false": {}
}
